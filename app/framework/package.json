{
  "name": "@storybook/framework",
  "version": "0.0.1",
  "description": "Storybook for framework: View framework components in isolation with hot reloading.",
  "keywords": [
    "storybook",
    "framework"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./preset": {
      "require": "./dist/preset.js",
      "import": "./dist/preset.mjs",
      "types": "./dist/preset.d.ts"
    },
    "./package.json": {
      "require": "./package.json",
      "import": "./package.json",
      "types": "./package.json"
    }
  },
  "homepage": "https://github.com/storybookjs/framework/app/framework",
  "bugs": {
    "url": "https://github.com/storybookjs/framework/issues"
  },
  "repository": {
    "type": "git",
    "directory": "app/framework"
  },
  "license": "MIT",
  "files": [
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "node ../../scripts/prepare.js"
  },
  "dependencies": {
    "@storybook/addons": "7.0.0-beta.9",
    "@storybook/builder-vite": "7.0.0-beta.9",
    "@storybook/channel-postmessage": "7.0.0-beta.9",
    "@storybook/channel-websocket": "7.0.0-beta.9",
    "@storybook/client-api": "7.0.0-beta.9",
    "@storybook/core-server": "7.0.0-beta.9",
    "@storybook/html": "7.0.0-beta.9",
    "@storybook/node-logger": "7.0.0-beta.9",
    "@storybook/preview-web": "7.0.0-beta.9",
    "magic-string": "^0.26.1"
  },
  "devDependencies": {
    "@types/node": "^16.0.0",
    "typescript": "~4.9.3"
  },
  "engines": {
    "node": "^14.18 || >=16"
  },
  "publishConfig": {
    "access": "public"
  }
}
